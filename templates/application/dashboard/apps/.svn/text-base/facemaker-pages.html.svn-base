
<div class="row rowBlack">
{include file="application/dashboard/widgets/shorter.html"}
 <div class="clear">
		<div class="col-lg-3">
        <div class="panel">
            <div class="row">
                <p class="announcement-heading"> </p>
                <p class="announcement-text"> </p>
            </div><!-- /.row -->
        </div><!-- /.panel -->
      </div><!-- /.col-lg-3 -->
      <div class="col-lg-3">
        <div class="panel">
            <div class="row">
                <p class="announcement-heading">{$totalentourage}</p>
                <p class="announcement-text"><span class="icons icon-text">&nbsp;</span><span class="labeltext">Total Unique Entourage</span></p>
            </div><!-- /.row -->
        </div><!-- /.panel -->
      </div><!-- /.col-lg-3 -->
      <div class="col-lg-3">
        <div class="panel">
            <div class="row">
                <p class="announcement-heading">{$totalplay}</p>
                <p class="announcement-text"><span class="icons icon-text">&nbsp;</span><span class="labeltext">Total Photo Taken</span></p>
            </div><!-- /.row -->
        </div><!-- /.panel -->
      </div><!-- /.col-lg-3 -->
      <div class="col-lg-3">
        <div class="panel">
            <div class="row">
                <p class="announcement-heading">{$totalwin}</p>
                <p class="announcement-text"><span class="icons icon-trophy">&nbsp;</span><span class="labeltext">Total Photo Submitted</span></p>
            </div><!-- /.row -->
        </div><!-- /.panel -->
      </div><!-- /.col-lg-3 -->
      
  </div><!-- /.clear -->
</div><!-- /.rowBlack -->

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-primary">
      <div class="panel-body">
          <table class="table table-bordered table-hover table-striped tablesorter">
            <thead>
              <tr class="blue">
                <th width="80" class="center"><h3><span class="icons icon-text">&nbsp;</span></h3></th>
                <th colspan="5"><h3>Facemaker</h3></th>
              </tr>
              <tr>
                <th class="center" width="80"><h3><span>No</span> <i class="fa fa-sort"></i></h3></th>
                <th><h3><span>{if $acts=='entourage'}Entourage Name{else}SBA Name{/if}</span> <i class="fa fa-sort"></i></h3></th>
                <th><h3><span>Total Photo Taken</span> <i class="fa fa-sort"></i></h3></th>
                <th><h3><span>Total Photo Submitted</span> <i class="fa fa-sort"></i></h3></th>
                <th><h3><span>Last Photo Taken (Timestamp)</span> <i class="fa fa-sort"></i></h3></th>
                <th><h3><span>Last Photo Submitted (Timestamp)</span> <i class="fa fa-sort"></i></h3></th>
              </tr>
            </thead>
            <tbody>
			{foreach from=$games key=k item=val  }
              <tr>
               <td class="center">{$val.no}</td>
					<td>{$val.names}</td>
					<td>{$val.totalplay}</td>
					<td>{$val.totalwin}</td>
					<td>{$val.totallose}</td>
					<td>{$val.datetimes}</td>
              </tr>
               {/foreach}
            </tbody>
          </table>
      </div><!-- /.panel-body -->
    </div><!-- /.panel-primary -->
     <ul class="pagination">
        <li class="disabled"><a href="#">&laquo;</a></li>
        <li class="active"><a href="#">1</a></li>
        <li><a href="#">2</a></li>
        <li><a href="#">3</a></li>
        <li><a href="#">4</a></li>
        <li><a href="#">5</a></li>
        <li><a href="#">&raquo;</a></li>
    </ul><!-- /.pagination -->
  </div><!-- /.col-lg-12 -->
</div><!-- /.row -->